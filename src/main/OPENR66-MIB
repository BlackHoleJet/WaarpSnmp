-- test notification
 
goldengatetraps  OBJECT IDENTIFIER ::= { goldengate 6 }

warningObject OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "A simple string object."
::= { warningGoldengate 1 }

warningGoldengate NOTIFICATION-TYPE
       OBJECTS { warningObject }
       STATUS current
       DESCRIPTION "Warning SMIv2 notification"
       ::= { goldengategroup 1 }
 
errorObject OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "A simple string object."
::= { errorGoldengate 1 }

errorGoldengate NOTIFICATION-TYPE
       OBJECTS { errorObject }
       STATUS current
       DESCRIPTION "Error SMIv2 notification"
       ::= { goldengategroup 2 }

overloadedObject OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "A simple string object."
::= { overloadedGoldengate 1 }

overloadedGoldengate NOTIFICATION-TYPE
       OBJECTS { overloadedObject }
       STATUS current
       DESCRIPTION "Overloaded SMIv2 notification"
       ::= { goldengategroup 3 }

shutdownObject OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "A simple string object."
::= { shutdownGoldengate 1 }

shutdownGoldengate NOTIFICATION-TYPE
       OBJECTS { shutdownObject }
       STATUS current
       DESCRIPTION "Shutdown SMIv2 notification"
       ::= { goldengategroup 4 }

goldengategroup NOTIFICATION-GROUP
    NOTIFICATIONS  { warningGoldengate,errorGoldengate,overloadedGoldengate,shutdownGoldengate }
    STATUS         current
    DESCRIPTION    "Group of Notification in GoldenGate"
    ::= { goldengatetraps 1 }
    
END
